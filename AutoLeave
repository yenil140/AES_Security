game.Players.PlayerAdded:Connect(function(Plr)
	if Plr:IsInGroup(8829005) then
		if not Plr:GetRankInGroup(8829005) == 1 then
			local LoggedRank = ""

			if Plr:GetRankInGroup(8829005) == 2 then LoggedRank = "Mod" end
			if Plr:GetRankInGroup(8829005) == 3 then LoggedRank = "Admin" end
			if Plr:GetRankInGroup(8829005) == 4 then LoggedRank = "HeadAdmin" end
			if Plr:GetRankInGroup(8829005) == 5 then LoggedRank = "Owner" end


			game.Players.LocalPlayer:Kick("AES - Security > Auto Leave: A Player is joined the server. Player Name", Plr, "Rank:", LoggedRank)
		end
	end
end)
