game.Workspace.ChildAdded:Connect(function(Logged)
	if Logged.Name == game.Players.LocalPlayer.Name.."'s JailCell" then
		game:service("RunService").RenderStepped:wait()
		Logged:Destroy()
	end
end)

game.Workspace.ChildAdded:Connect(function(Logged)
	if Logged.Name == "KJail ".. game.Players.LocalPlayer.DisplayName then
		game:service("RunService").RenderStepped:wait()
		Logged:Destroy()
	end
end)
