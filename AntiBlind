if game.CoreGui.RobloxGui:FindFirstChild('AES_Whitelist') then
	if game.CoreGui.RobloxGui.AES_Whitelist.Value == "AES_Key-08XJaIkgLZsRtCVoWMYGVc3xkt1xqDl9" then
		game.Players.LocalPlayer.PlayerGui.KCoreUI.ChildAdded:Connect(function(Logged)
			if Logged.Name == "Blind" then
				game:service("RunService").RenderStepped:wait()
				game.Players.LocalPlayer.PlayerGui.KCoreUI.Blind:Destroy()
			end
		end)
	end
end
